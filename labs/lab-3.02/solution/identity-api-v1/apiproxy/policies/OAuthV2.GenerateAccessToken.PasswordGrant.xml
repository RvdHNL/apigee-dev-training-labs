<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="OAuthV2.GenerateAccessToken.PasswordGrant">
  <Operation>GenerateAccessToken</Operation>
  <ExpiresIn ref="config.accessTokenExpiry">3600000</ExpiresIn>
  <RefreshTokenExpiresIn ref="config.refreshTokenExpiry">2592000000</RefreshTokenExpiresIn>
  <SupportedGrantTypes>
    <GrantType>password</GrantType>
  </SupportedGrantTypes>
  <Attributes>
    <Attribute name="userId" ref="userInfo.id"/>
  </Attributes>
  <GenerateResponse enabled="false"/>
</OAuthV2>